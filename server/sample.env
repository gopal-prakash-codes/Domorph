# Server Configuration
PORT=5001
NODE_ENV=development

# Client URLs (CORS)
CLIENT_URL=http://127.0.0.1:5173
DEV_URL=http://127.0.0.1:5001
EXTRA_URL=127.0.0.1

# Scraper Configuration
CLIENT_DIR_PATH=../client/public
WEBSITE_BASE_PATH=/scraped_website
WEBSITE_DIR=scraped_website
CONCURRENCY_LIMIT=10
PUPPETEER_ARGS=--no-sandbox,--disable-setuid-sandbox

# LLM API Keys
ANTHROPIC_API_KEY=XXXXX
OPENAI_API_KEY=XXXXXX

# Vercel v0 API - Get key from https://v0.dev/dashboard
# Required for screenshot-to-code feature
V0_API_KEY=XXXXXX
